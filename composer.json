{
    "name": "arimolzer/ifttt-voip",
    "description": "",
    "keywords": [
        "arimolzer",
        "ifttt-voip"
    ],
    "homepage": "https://github.com/arimolzer/ifttt-voip",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ari Molzer",
            "email": "ari.molzer@molzertech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Arimolzer\\IftttVoip\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arimolzer\\IftttVoip\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arimolzer\\IftttVoip\\IftttVoipServiceProvider"
            ],
            "aliases": {
                "IftttVoip": "Arimolzer\\IftttVoip\\IftttVoipFacade"
            }
        }
    }
}
